User Function MT450QRY()

	Local c_Ret := paramixb[1]
	
	If VERROT("A455LibAut") .and. (MV_PAR01 <> MV_PAR02 .or. MV_PAR02 <> SC9->C9_PEDIDO .or. MV_PAR01 <> SC9->C9_PEDIDO)
		c_Ret += " and C9_PEDIDO = '" + SC9->C9_PEDIDO + "' "
		msgAlert("O ๚nico pedido a ser liberado serแ o de n๚mero: " + SC9->C9_PEDIDO + ". Certifique-se que esse ้ o pedido selecionado no browser.", "A T E N ว ร O")
	EndIf
	
Return c_Ret
