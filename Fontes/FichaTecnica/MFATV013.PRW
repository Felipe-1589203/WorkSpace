User Function MFATV013()
Local lRet   := .T.                         
Local lObrig := SuperGetMv("MV_ALTPOBR",.F.,.T.,)


If VerRot('MFATA012') .or. VerRot('U_MFATA012')
	If M->Z4_ALTURA < 2000.Or.M->Z4_ALTURA >2200
	   Msgbox("Coloque uma altura vแlida entre 2000 e 2100","Altura da Porta","ALERT")
	   If lObrig
	      lRet := .F.
	   EndIf   
	EndIf   
Else
	If type('M->Z0_ALTURA') = 'N'
		If M->Z0_ALTURA < 2000.Or.M->Z0_ALTURA >2200
		   Msgbox("Coloque uma altura vแlida entre 2000 e 2100","Altura da Porta","ALERT")
		   If lObrig
		      lRet := .F.
		   EndIf   
		EndIf   
	EndIf
EndIf

Return(lRet)
